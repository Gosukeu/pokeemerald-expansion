// pour chaque event associer une var, pour ne pas pouvroir battre le meme gars 6 fois!
script SchoolHouseTournamentTrigger {
	lockall
	applymovement(8, exlamationmovement)
	waitmovement(8)
	applymovement(OBJ_EVENT_ID_PLAYER, exlamationmovement)
	waitmovement(OBJ_EVENT_ID_PLAYER)
	msgbox(format("Ah, I see... You must be the last student! Come over here-you're just in time. We are about to begin the grand Tournament of Knowledge and Battle! The winner gets a mysterie egg!"), MSGBOX_AUTOCLOSE)
	applymovement(7, oldpapygoesback)
	waitmovement(7)
	removeobject(7)
	setvar(VAR_ROUTE107_STATE, 2)
	releaseall
	end
}


