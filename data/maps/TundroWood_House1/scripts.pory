script KID1TOURNEMENTQUIZ {
    if(var(VAR_EACH_TRAINER) == 0){

	if(var(VAR_SCHOOL_TOURNAMENT) == 0){
		lockall
		faceplayer
		msgbox(format("Hah! I will be the first to win this tournament! I've studied every type, every move… You don’t stand a chance against me! Get ready for an unforgettable lesson!"), MSGBOX_AUTOCLOSE)
	    msgbox(format("Quiz Time!"), MSGBOX_AUTOCLOSE)
		msgbox(format("Pikachu is an Electric-type Pokémon. Yes or No?"), MSGBOX_YESNO)
		if(var(VAR_RESULT) == YES){
			msgbox(format("Well done! Now let's fight!"), MSGBOX_AUTOCLOSE)
			call(KID1TOURNEMENTBattle1)
		}
		if(var(VAR_RESULT) == NO){
			msgbox(format("That's not right try again."), MSGBOX_AUTOCLOSE)
			releaseall
		}

	}
	if(var(VAR_SCHOOL_TOURNAMENT) == 1){
		lockall
		faceplayer
		msgbox(format("Hmm… You managed to pass the first round? Interesting. But the real challenge starts now. A great Trainer doesn’t rely only on strength but also on strategy! Let’s see if you’re up to the task."), MSGBOX_AUTOCLOSE)
	    msgbox(format("Quiz Time!"), MSGBOX_AUTOCLOSE)
        msgbox(format("Can a Pokémon have more than two active status conditions at the same time? Yes or No?"), MSGBOX_YESNO)
		if(var(VAR_RESULT) == NO){
			msgbox(format("Well done! Now let's fight!"), MSGBOX_AUTOCLOSE)
			call(KID1TOURNEMENTBattle2)

		}
		if(var(VAR_RESULT) == YES){
			msgbox(format("That's not right try again."), MSGBOX_AUTOCLOSE)
			releaseall
		}
	}
	if(var(VAR_SCHOOL_TOURNAMENT) == 2){
		lockall
		faceplayer
		msgbox(format("You’re stronger than I thought... But I can’t give up now! If I lose, how can I prove that all my training was worth it? Alright, I’ll give it my all!"), MSGBOX_AUTOCLOSE)
		msgbox(format("Quiz Time!"), MSGBOX_AUTOCLOSE)
		 msgbox(format("A Water-type Pokémon can be burned by a Fire-type move. Yes or No?"), MSGBOX_YESNO)
		if(var(VAR_RESULT) == NO){
			msgbox(format("Well done! Now let's fight!"), MSGBOX_AUTOCLOSE)
			call(KID1TOURNEMENTBattle3)
		}
		if(var(VAR_RESULT) == YES){
			msgbox(format("That's not right try again."), MSGBOX_AUTOCLOSE)
			releaseall
		}
	}
	if(var(VAR_SCHOOL_TOURNAMENT) == 3){
		lockall
		faceplayer
		msgbox(format("We finally meet… I knew you would make it this far. But I’ve also given everything to reach this moment! This battle will decide who truly deserves a spot in the final. Don’t hold back!"), MSGBOX_AUTOCLOSE)
		msgbox(format("Quiz Time!"), MSGBOX_AUTOCLOSE)
		 msgbox(format("The ability Intimidate lowers the opponent’s Special Attack. Yes or No?"), MSGBOX_YESNO)
		if(var(VAR_RESULT) == NO){
			msgbox(format("Well done! Now let's fight!"), MSGBOX_AUTOCLOSE)
			call(KID1TOURNEMENTBattle4)
		}
		if(var(VAR_RESULT) == YES){
			msgbox(format("That's not right try again."), MSGBOX_AUTOCLOSE)
			releaseall
		}
	}
	if(var(VAR_SCHOOL_TOURNAMENT) == 4){
		lockall
		faceplayer
		msgbox(format("Impressive. Very impressive. Few students make it this far… But I am the best for a reason. This tournament is not just about strength, but also knowledge and mastery. Are you ready to face the Academy’s greatest challenge?"), MSGBOX_AUTOCLOSE)
		msgbox(format("Quiz Time!"), MSGBOX_AUTOCLOSE)
		 msgbox(format("A Pokémon can have more than four moves in battle. Yes or No?"), MSGBOX_YESNO)
		if(var(VAR_RESULT) == NO){
			msgbox(format("Well done! Now let's fight!"), MSGBOX_AUTOCLOSE)
			call(KID1TOURNEMENTBattle5)
		}
		if(var(VAR_RESULT) == YES){
			msgbox(format("That's not right try again."), MSGBOX_AUTOCLOSE)
			releaseall
		}
	}
	if(var(VAR_SCHOOL_TOURNAMENT) == 5){
		lockall
		faceplayer
		msgbox(format("Incredible! You have proven yourself not only as an outstanding Trainer but also as a true Pokémon scholar. On behalf of the entire Academy, I congratulate you. Here is your reward, Champion of the Tournament of Knowledge and Battle!"), MSGBOX_AUTOCLOSE)
		msgbox(format("Quiz Time!"), MSGBOX_AUTOCLOSE)
		 msgbox(format("Charizard is a Dragon-type Pokémon. Yes or No?"), MSGBOX_YESNO)
		if(var(VAR_RESULT) == YES){
			msgbox(format("Well done! Now let's fight!"), MSGBOX_AUTOCLOSE)
			call(KID1TOURNEMENTBattle6)
		}
		if(var(VAR_RESULT) == NO){
			msgbox(format("That's not right try again."), MSGBOX_AUTOCLOSE)
			releaseall
		}
	    
	}
	
}
else{
	lockall
	msgbox(format("I already lost, their more participants!"))
}
  
}
